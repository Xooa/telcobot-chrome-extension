{
  "manifest_version": 3,
  "name": "Telcobot",
  "version": "1.5",
  "description": "This is a chrome extension to inject a telcobot chatbot on any pages",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "permissions": [
    "webRequest",
    "storage"
  ],
  "action": {
    "default_popup": "options.html",
    "default_icon": {
      "16": "images/icon.svg",
      "32": "images/icon.svg",
      "48": "images/icon.svg",
      "128": "images/icon.svg"
    }
  },
  "icons": {
    "16": "images/icon.svg",
    "32": "images/icon.svg",
    "48": "images/icon.svg",
    "128": "images/icon.svg"
  }
}